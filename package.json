{
  "name": "bs-auto-dark-mode",
  "displayName": "auto dark mode",
  "description": "",
  "version": "0.0.19",
  "publisher": "ColoryPanda",
  "icon": "src/assets/images/icon.png",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:boss.auto-dark-mode",
    "onCommand:boss.replace-color"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "icons": {
      "icon-moon": {
        "description": "generate dark mode",
        "default": {
          "fontPath": "src/assets/fonts/remixicon.woff",
          "fontCharacter": "\\ebd4"
        }
      },
      "icon-replace": {
        "description": "replace color",
        "default": {
          "fontPath": "src/assets/fonts/remixicon.woff",
          "fontCharacter": "\\efc0"
        }
      }
    },
    "commands": [
      {
        "command": "boss.auto-dark-mode",
        "title": "Auto Dark Mode",
        "icon": "$(icon-moon)"
      },
      {
        "command": "boss.replace-color",
        "title": "Replace Color",
        "icon": "$(icon-replace)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "editorLangId == vue",
          "command": "boss.replace-color",
          "group": "navigation@-2"
        },
        {
          "when": "editorLangId == vue",
          "command": "boss.auto-dark-mode",
          "group": "navigation@-1"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/postcss-less": "^4.0.2",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "postcss": "^8.4.16",
    "postcss-less": "^6.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
